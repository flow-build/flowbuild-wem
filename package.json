{
  "name": "flowbuild-wem",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts)\"",
    "test": "jest",
    "test:cov": "pnpm run test --coverage --forceExit",
    "test:badges": "pnpm run test:cov  && jest-coverage-badges",
    "start": "node ./dist/src/server",
    "start:dev": "ts-node -r tsconfig-paths/register ./src/server.ts",
    "start:climem": "export CLIMEM=8990 && pnpm run build && node -r climem -r tsconfig-paths/register ./dist/src/server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "climem": "^1.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.3.0",
    "fastify": "^4.12.0",
    "axios": "^1.3.4",
    "bullmq": "^3.10.1",
    "kafkajs": "^2.2.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "node-cache": "^5.1.2"
  }
}